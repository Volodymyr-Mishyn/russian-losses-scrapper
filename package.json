{
  "name": "russian-losses-scrapper",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "test": "npx jest",
    "build:dev": "tsc",
    "build:prod": "tsc -p tsconfig.prod.json",
    "format": "npx prettier --write 'src/**/*.ts'",
    "dev:mod:nh": "ts-node ./src/index.ts --source=mod --notHeadless",
    "dev:oryx:russia": "ts-node ./src/index.ts --source=oryx --oryxType=Russia --notHeadless",
    "dev:mod:full": "ts-node ./src/index.ts --full --source=mod --notHeadless",
    "start:mod": "ts-node ./src/index.ts --source=mod",
    "start:mod:full": "ts-node ./src/index.ts --full --source=mod",
    "start:mod:file": "ts-node ./src/index.ts --source=mod --output=file --outputPath=mod.txt",
    "start:mod:http": "ts-node ./src/index.ts --source=mod --output=http --outputPath=someurl.com",
    "start:mod:process": "ts-node ./src/index.ts --source=mod --output=process --outputPath=out",
    "start:mod:file:full": "ts-node ./src/index.ts --source=mod --full --output=file --outputPath=mod.txt",
    "start:mod:http:full": "ts-node ./src/index.ts --source=mod --full --output=http --outputPath=someurl.com",
    "start:mod:process:full": "ts-node ./src/index.ts --source=mod --full --output=process --outputPath=out",
    "start:oryx:russia": "ts-node ./src/index.ts --source=oryx --oryxType=Russia",
    "start:oryx:russia:file": "ts-node ./src/index.ts --source=oryx --oryxType=Russia --output=file --outputPath=oryx.txt",
    "start:oryx:russia:http": "ts-node ./src/index.ts --source=oryx --oryxType=Russia --output=http --outputPath=someurl.com",
    "start:oryx:russia:process": "ts-node ./src/index.ts --source=oryx --oryxType=Russia --output=process --outputPath=out",
    "start:oryx:russia:file:nh": "ts-node ./src/index.ts --source=oryx --oryxType=Russia --output=file --outputPath=oryx.txt --notHeadless",
    "start:oryx:ukraine:file:nh": "ts-node ./src/index.ts --source=oryx --oryxType=Ukraine --output=file --outputPath=oryx-ukraine.txt --notHeadless"
  },
  "author": "Volodymyr Mishyn",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.5.1",
    "cheerio": "^1.0.0-rc.12",
    "lodash": "^4.17.21",
    "minimist": "^1.2.8",
    "puppeteer": "^21.6.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.6",
    "@types/jsdom": "^21.1.4",
    "@types/lodash": "^4.14.200",
    "@types/node": "^18.7.13",
    "@types/puppeteer": "^7.0.4",
    "concurrently": "^7.3.0",
    "husky": "^8.0.1",
    "jest": "^29.7.0",
    "jest-puppeteer": "^9.0.1",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^5.3.3"
  }
}
